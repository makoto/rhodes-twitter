<ul id="MyTimeLines" title="MyTimeLines">
<%@MyTimeLines.each do |x|%>

<li class ="row">
  <div class ="photo">
    <img src=<%= escape x.user_profile_image_url %> alt=<%= escape x.user_screen_name %> />
  </div>
  <div class ="status">
    <div class ="text">
     <%= escape x.text %>
    </div>
    <div class ="user">
     <%= x.user_name %> <%= parse_time x.created_at %> via <%= escape x.source %>
    </div>
  </div>
</li>
<%end%>
<li><font color="blue"><%=link_to "New MyTimeLine", :action => :new%></font></li>
</ul>